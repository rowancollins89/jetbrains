<templateSet group="user">
  <template name="story" value="export default {&#10;    title: '$title$',&#10;};&#10;&#10;export const $storyName$ = () =&gt; ({&#10;    template: `$template$`,&#10;});&#10;    &#10;$storyName$.story = {&#10;    name: '$name$',&#10;    &#10;    components: { $componentName$ },&#10;};" description="Storybook (CSF) story" toReformat="true" toShortenFQNames="true">
    <variable name="title" expression="&quot;title&quot;" defaultValue="title" alwaysStopAt="true" />
    <variable name="storyName" expression="&quot;storyName&quot;" defaultValue="storyName" alwaysStopAt="true" />
    <variable name="template" expression="&quot;template&quot;" defaultValue="template" alwaysStopAt="true" />
    <variable name="name" expression="&quot;default&quot;" defaultValue="name" alwaysStopAt="true" />
    <variable name="componentName" expression="&quot;componentName&quot;" defaultValue="componentName" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>